{
    "title": "Auth0 WeChat Proxy Extension",
    "name": "auth0-WeChat-extension",
    "version": "1.0.1",
    "author": "Auth0",
    "useHashName": false,
    "description": "WeChat proxy endpoint to support private key jwt / client assertion",
    "type": "application",
    "category": "end_user",
    "logoUrl": "https://app.WeChat.gov.sg/static/og_image.png",
    "initialUrlPath": "/",
    "keywords": [
      "auth0",
      "extension",
      "WeChat"
    ],
    "secrets": {
      "AUTH0_CUSTOM_DOMAIN": {
        "description": "YOUR_CUSTOM_DOMAIN",
        "example": "auth.oauth101.net",
        "required": true
      },
      "AUTH0_CLIENT_ID": {
        "description": "Auth0 Client ID",
        "required": true
      },
      "AUTH0_CLIENT_SECRET": {
        "description": "Client Secret",
        "required": true
      },
      "WeChat_SIGNING_TYPE": {
        "description": "Signing TYPE",
        "type": "select",
        "allowMultiple": false,
        "default": "Mini",
        "options": [
          {
            "value": "Mini",
            "text": "Mini"
          },
          {
            "value": "PC_WEB",
            "text": "PC_WEB"
          }
        ]
      },
      "WeChat_CLIENT_ID": {
        "description": "WeChat Registered client_id",
        "required": true
      },
      "WeChat_CLIENT_SECRET": {
        "description": "Client Secret",
        "required": true
      }
    }
  }
